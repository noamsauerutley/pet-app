<h1>Create your account</h1>

<%= form_for @adopter do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :description%>
<%= f.text_area :description %>
<%= f.label :zip %>
<%= f.text_field :zip %>
<%= f.label :image_url %>
<%= f.text_field :image_url %>
<%= f.password_field :password %>
<%= f.submit %>
<% end%>
